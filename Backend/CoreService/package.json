{
  "name": "core-backend-service",
  "version": "1.0.0",
  "description": "Core Backend Service for Enterprise Application",
  "keywords": [
    "backend",
    "api",
    "authentication",
    "navigation",
    "footer",
    "dotnet",
    "aspnetcore"
  ],
  "author": "Your Company",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "your-repo-url"
  },
  "scripts": {
    "dev": "dotnet watch run",
    "build": "dotnet build",
    "build-release": "dotnet build -c Release",
    "start": "dotnet run",
    "publish": "dotnet publish -c Release -o ./publish",
    "test": "dotnet test"
  },
  "framework": ".NET 9",
  "language": "C#",
  "runtime": "ASP.NET Core",
  "port": 7100,
  "endpoints": {
    "authentication": "/api/auth",
    "navigation": "/api/navigation",
    "footer": "/api/footer"
  },
  "features": [
    "JWT Authentication",
    "Role-Based Access Control",
    "Navigation Management",
    "Footer Management",
    "User Management",
    "Dynamic Content"
  ],
  "dependencies": {
    "Microsoft.AspNetCore.Authentication.JwtBearer": "9.0.0",
    "System.IdentityModel.Tokens.Jwt": "latest"
  }
}

